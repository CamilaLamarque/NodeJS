var nodemailer = require('nodemailer');

var transporter = nodemailer.createTransport({
  service: 'gmail',
  auth: {
    user: 'nicolyquerinodesouza2023@gmail.com',
    pass: 'vfrs mrqj jglr onsq'
  }
});

var mailOptions = {
  from: 'nicolyquerinodesouza2023@gmail.com',
  to: 'nicolyquerinodesouza2007@gmail.com',
  subject: 'Sending Email using Node.js',
  text: 'uuuuuuuuuuuuu!'
};

transporter.sendMail(mailOptions, function(error, info){
  if (error) {
    console.log(error);
  } else {
    console.log('Email sent: ' + info.response);
  }
}); 